#!/usr/bin/env bash

function main {
  case "${1:-}" in
    login|assume)
      local nm_cmd="$1"; shift
      exec fogg "$nm_cmd" "${BASH_SOURCE##*/}" "$@"
      ;;
    *)
      if [[ "$#" == 0 ]]; then
        exec fogg assume "${BASH_SOURCE##*/}" "$@"
      else
        exec fogg env "${BASH_SOURCE##*/}" "$@"
      fi
      ;;
  esac
}

unset AWS_VAULT
source sub-chain imma "$@"
